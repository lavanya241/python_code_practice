def fbnum(num1, num2):
    if num1 > num2:
        return num1
    elif num2 > num1:
        return num2
    else:
        return "Both numbers are equal"

num1 = 15
num2 = 20
print(f"The bigger number is:{fbnum(num1,num2)}")

def cfbnum(num1,num2):
    if num1 < num2:
        return True
    else:
        return False

    if num1 <= num2:
        return True
    else:
        return False

    if num1 > num2:
        return True
    else:
        return False

    if num1 >= num2:
        return True
    else:
        return False

    if num1 == num2:
        return True
    else:
        return False

    if num1 != num2:
        return True
    else:
        return False
Print(cfbnum(num1,num2))




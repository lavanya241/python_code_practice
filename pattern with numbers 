n = 5
for i in range(1, n+1):
    for j in range(n-i):
        print(" ", end=" ")  # Printing spaces
    for j in range(1, i+1):
        print(j, end=" ")  # Printing numbers in increasing order
    for j in range(i-1, 0, -1):
        print(j, end=" ")  # Printing numbers in decreasing order
    print()
